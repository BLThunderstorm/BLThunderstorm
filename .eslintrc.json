{
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "sourceType": "module",
    },
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/essential",
        "standard",
        '@nuxtjs/eslint-config-typescript'
  
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "parser": "@typescript-eslint/parser",
        "sourceType": "module"
    },
    "plugins": [
        "vue",
        "@typescript-eslint"
        "@babel"
    ],
    "rules": {
        "vue/require-v-for-key":"off"
    }
}
